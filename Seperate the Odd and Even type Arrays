package com.arrays;

public class array {
	public static void main(String[] args) {
		int[] array = {5, 6, 7,8,9,10};
		System.out.print("Even numbers is :");
		for(int number : array) {
			
			
			if (number%2==0)
			{
				System.out.print(number+" ");
			}
		}
		
		System.out.println();
		
		System.out.print("odd numbers is :");
		for (int number : array )
		{
			
			if(number%2!=0 )
			{
				System.out.print(number + " ");
			}
		
		
		}
	
	          }
		}
		
			
